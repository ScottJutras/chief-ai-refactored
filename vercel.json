{
  "version": 2,
  "functions": {
    "api/webhook.js": {
      "memory": 2048,
      "maxDuration": 15,
      "excludeFiles": "{docs/**,public/**,dashboard/**,parse/**,deep-dive/**,api/index-sops.js,api/index.js,node_modules/pdfkit/**,node_modules/exceljs/**,node_modules/@google-cloud/**,node_modules/google-auth-library/**}"
    },
    "api/index-sops.js": {
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles": "docs/howto/**"
    },
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/ping.js": {
      "memory": 256,
      "maxDuration": 5
    }
  },
  "routes": [
    { "src": "^/api/index-sops$", "dest": "/api/index-sops.js" },
    { "src": "^/api/webhook/reminders/cron/(.*)$", "dest": "/api/webhook.js" },
    { "src": "^/api/webhook$", "dest": "/api/webhook.js" },
    { "src": "^/webhook$", "dest": "/api/webhook.js" },
    { "src": "^/api/ping$", "dest": "/api/ping.js" },
    { "src": "^/(.*)$", "dest": "/api/index.js" }
  ],
  "crons": [
    {
      "path": "/api/webhook/reminders/cron/4bf3e4de8ccc4713119b79959f0ac990af24ca2ea5e348ff462b1d7958ae424e",
      "schedule": "* * * * *"
    }
  ]
}
