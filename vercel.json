{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "api/index-sops.js", "use": "@vercel/node" },
    { "src": "api/webhook.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/index-sops", "dest": "/api/index-sops.js" },
    { "src": "/api/webhook", "dest": "/api/webhook.js" },
    { "src": "/api/webhook/reminders/cron/.*", "dest": "/api/webhook.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ],
  "crons": [
    {
      "path": "/api/webhook/reminders/cron/4bf3e4de8ccc4713119b79959f0ac990af24ca2ea5e348ff462b1d7958ae424e",
      "schedule": "* * * * *"
    }
  ]
}