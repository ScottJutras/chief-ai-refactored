{
  "version": 2,
  "files": [
    "index.js",
    "package.json",
    "package-lock.json",

    "routes/**",
    "handlers/**",
    "middleware/**",
    "models/**",
    "nlp/**",
    "services/**",
    "utils/**",
    "config/**",
    "public/**",

    ".vercelignore"
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "methods": ["GET","POST"],
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
